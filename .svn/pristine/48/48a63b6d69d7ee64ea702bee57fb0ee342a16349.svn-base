


#C_cloud_section1 {  width: 100%;  position: relative;bottom: 50px;transition: 2s;opacity: 0}
.C_cloud_section_transform{ transform: translateY(30px)}
.middle_Section {  width: 80%;  max-width: 1000px;  margin: auto;  text-align: center}
#C_cloud_section1>div>div:first-child{ width: 100%; height: 255px;}
#C_cloud_section1 div[class="panel-body"]>p{  padding: 0px 0px 9px 0px; text-align: left;  }
#C_cloud_section2{position: relative;bottom: 50px; transition: 2s;opacity: 0}
#C_cloud_section2 .panel-heading:first-child .panel-title>h1{font-size: 40px;padding-bottom: 0px;margin-top: 60px;}
#C_cloud_section2 >.middle_Section>.panel-heading>.panel-title> p {  color: #979797;font-size: 16px; margin-bottom: 25px;}
#C_cloud_section2>.middle_Section>.row>.col-md-6>p:first-child{font-size: 12px;}
#C_cloud_section2>.middle_Section>.row>.col-md-6>h3{font-size: 20px;}
#C_cloud_section2>.middle_Section>.row>.col-md-6>p:last-of-type{font-size: 13px;padding-right: 10px;padding-left: 10px;}
#C_cloud_section2>.middle_Section>.row>.col-md-6>button{background-color: #17a085;color: white}
#C_cloud_section3 {  width: 100%;  }
#C_cloud_section3 .panel-title >p{  color: #979797;font-size: 17px;       }
