#C_cloud_section3 .panel-body>button:first-child{  width: 100%;  background-color: #16a085;  color: white;  height: 54px;  }
.C_cloud_showandhidden{transition-delay: 0.5s;  animation-name:show_hidden;  animation-duration: 0.75s;  animation-iteration-count: 3;  }
@keyframes show_hidden { 0%   {opacity: 1} 50%  {opacity: 0}  100% {opacity: 1}        }
