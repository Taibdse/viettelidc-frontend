.qaDivSmall .btnactive {background:#066;color:white;font-size:36px;box-sizing:border-box;}
.qaDivSmall .btnactive i {height:42px; line-height:42px;position:relative; top:25%;left: 50%; transform:translate(-50%,-50%); font-size:15px}
#move{transform: translateY(-10px);position: relative;animation-duration: 2s}
.move{transform: translateY(20px);animation-timing-function: ease-in-out;}
.qaDivSmall .dropbtn:hover {cursor:pointer;}
.qaDiv .dropdownContent {background:white;display:none;overflow:hidden;padding-left:14px;font-size:12px;font-weight:450;}
.qaInfo {background:white;width:100%;border:none;}
