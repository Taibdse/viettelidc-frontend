body {padding: 0px;
    margin: 0px
}
.center{width: 80%;text-align: center;margin-right: auto;margin-left: auto; max-width: 1080px}

.logowarp{width: 18%; height: 55px; float: left;padding-top: 10px}
.tagwarp{width: 82%; height: 55px;float: left}
.menu{width: 100%;  background: white; border-bottom: solid 3px #f05a3a; padding: 0px ;position: fixed; z-index: 2}
#menulogo{height: 35px; float: left}
.menulogo{height: 35px; float: left}
.menutag{ font-size: 14px; text-align: center; border-radius: 5px 5px 0px 0px; float: left; padding: 18px 8px; height: 55px;float: right }
.menutag:hover{background: lightgrey}
.active{background:lightgrey }
.navmenu{height: 80px;margin: 0px;padding: 20px 40px;overflow:hidden;box-sizing:border-box;}
#navmenuin{display: none;overflow:hidden;width: 100%; transition: 0.3s; background: #252525;font-size: 14px;color: #999999;padding-left: 50px;padding-right: 50px}
#navmenuin .dropdown {width:100%;float:left;box-sizing:border-box;}
.submenu1{padding-left:10px;display:none;}
.menuintag{float: left;width: 100%;color:  #999999;text-decoration: none;border-bottom: solid 0.5px #999999;padding-bottom: 10px;padding-top: 10px}
.menuintag i {color:#09C; float:right;margin-right:10px;}
.menuintag:hover{color: white;text-decoration: none}
#navmenuin i:hover {color:white;cursor:pointer;}
#navmenuin .dropdown:hover .submenu1 {display:block;}
.show {display: block}

.menutag .container-fluid { width: 60%; }
.dropdownmn {transform: translateX(-50%);display: none; position:fixed;left:50%; background:white; box-shadow: 0px 8px 16px 0px rgba(0,0,0,0.2); z-index:2;margin-top: 19px;}
.menutag:hover .dropdownmn{display: block}
.dropmenuDiv {background:inherit;text-align:left;padding-top:20px; box-sizing:border-box;display:inline-block;}
.dropmenuDiv h3 {
    font-size:18px;
    color:#099;
    margin-bottom:25px;
    border-bottom:1px solid #C1C1C1;
    padding-bottom:25px;
}
.dropmenuDiv ul {list-style-type:none;padding-left:0px;}
.dropmenuDiv ul li {padding-right:30px;transition:all 0.3s;padding-top:2px; padding-bottom:2px;}
.dropmenuDiv ul li h4 {color: orange; font-size:15px;}
.dropmenuDiv ul li h4 a {text-decoration:none;color:orange;}
.dropmenuDiv ul li h4 i {font-size:25px; margin-right:15px;}
.dropmenuDiv ul li p {color:#7D7D7D; font-size:11px;}
.dropmenuDiv ul li:hover {background-color:#E3E1E8;cursor:pointer; opacity:0.9;}
.dropmenuDiv ul li:hover h4 {color:#0069D2;}
.dropmenuDiv ul li:hover h4 a {color:#0069D2;}
.dropmenuDiv .ulThongTin li {padding-right:0px;}

.centerdiv{width: 80%;max-width: 1080px}
